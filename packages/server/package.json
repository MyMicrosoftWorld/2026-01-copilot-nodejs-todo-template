{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "ts-node ./src/server.ts | pino-pretty",
    "start:prod": "node ./lib/server.js",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "@azure/cosmos": "^4.9.0",
    "express": "^5.2.1",
    "pino-http": "^11.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "jest": "^30.2.0",
    "pino-pretty": "^13.1.3",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "jest": {
    "preset": "ts-jest/presets/default-esm",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "lib/**/*.js"
    ],
    "verbose": true
  }
}
